    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/server/server.js",
            "cwd": "${workspaceRoot}",
            "outFiles": [],
            "sourceMaps": true,
            "env": {
                "NODE_ENV": "development",
                "LOGSQL": "false",
                "USE_COMPRESSION": "false",
            },
            "console": "internalConsole"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 3000,
            "outFiles": [],
            "sourceMaps": true
        }
    ]
}